{
  "name": "Quick QR",
  "version": "1.1",
  "manifest_version": 3,
  "description": "QR code generator for web pages",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "downloads"
  ]
}